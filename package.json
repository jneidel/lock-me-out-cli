{
  "name": "lock-me-out-cli",
  "version": "0.0.1",
  "description": "Temporarily lock yourself out, by encrypting e.g. your netflix password for a week",
  "repository": "jneidel/lock-me-out-cli",
  "license": "MIT",
  "author": {
    "name": "Jonathan Neidel",
    "email": "github@jneidel.com",
    "url": "https://jneidel.com"
  },
  "scripts": {
    "test": "ava",
    "testing": "ava -w",
    "unit": "ava -w --match='*[unit]*'"
  },
  "main": "bin/cli.js",
  "bin": {
    "lock-me-out": "bin/cli.js",
    "lmo": "bin/cli.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "dayjs": "^1.8.0",
    "inquirer": "6.0.0",
    "lock-me-out-api": "^0.1.0",
    "log-update": "^2.3.0",
    "meow": "^5.0.0",
    "update-check": "^1.5.2"
  },
  "devDependencies": {
    "ava": "latest",
    "nodemon": "latest"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ]
  },
  "keywords": [
    "lmo"
  ]
}
